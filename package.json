    "nanoid": "^5.1.2",
    "next": "15.2.0",
    "next-sitemap": "^4.2.3",
    "payload": "3.26.0",
    "react": "19.0.0",
    "react-day-picker": "^8.10.1",
    "react-dom": "19.0.0",
    "react-hook-form": "^7.54.2",
    "resend": "^4.1.2",
