{
  "extends": ["stylelint-config-standard", "stylelint-config-clean-order"],
  "rules": {
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["apply", "layer", "responsive", "screen", "tailwind", "variants"]
      }
    ],
    "no-descending-specificity": null,
    "selector-class-pattern": null
  }
}
